{
  "name": "hyacinthattendance",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:emulator": "firebase emulators:start",
    "dev:full": "concurrently \"npm run dev\" \"npm run dev:emulator\"",
    "build": "concurrently \"vite build\" \"npm run build:hyacinthattendance\" \"npm run build:hyattend\"",
    "build:prod": "vite build --mode production",
    "build:hyacinthattendance": "vite build --outDir dist-hyacinthattendance",
    "build:hyattend": "vite build --outDir dist-hyattend",
    "lint": "eslint .",
    "preview": "vite preview",
    "deploy": "npm run build:prod && firebase deploy"
  },
  "dependencies": {
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "firebase": "^11.3.0",
    "phosphor-react": "^1.4.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router-dom": "^7.1.5",
    "recharts": "^2.15.1",
    "styled-components": "^6.1.15",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.19.0",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@vitejs/plugin-react": "^4.3.4",
    "concurrently": "^8.2.2",
    "eslint": "^9.19.0",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.18",
    "globals": "^15.14.0",
    "vite": "^6.1.0"
  }
}
